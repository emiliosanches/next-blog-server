{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./dist",
    
    "strict": true,
    "strictPropertyInitialization": false,
    "downlevelIteration": true,
    
    "esModuleInterop": true,
    "resolveJsonModule": true,
    
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules"
  ]
}
